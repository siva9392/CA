\\ADDITION 
#include <stdio.h>

int main() {
    int num1 = 2;
    int num2 = 3;
    int sum;

    sum = num1 + num2;
    printf("The sum of %d and %d is %d\n", num1, num2, sum);

    return 0;
}

\\SUBTRACTION:

#include <stdio.h>

int main() {
    int num1 = 5;
    int num2 = 3;
    int difference;
    difference = num1 - num2;
    printf("The difference between %d and %d is %d\n", num1, num2, difference);

    return 0;
}

\\MULTIPLICATION:

#include <stdio.h>

int main() {
    int num1 = 5;
    int num2 = 3;
    int product;
    product = num1 * num2;
    printf("The product of %d and %d is %d\n", num1, num2, product);

    return 0;
}

\\DIVISION:

#include <stdio.h>

int main() {
    int dividend = 10;
    int divisor = 2;
    int quotient;
    quotient = dividend / divisor;
    printf("The quotient of %d divided by %d is %d\n", dividend, divisor, quotient);

    return 0;
}
